{
  "name": "node-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "babel-node app",
    "migrate": "sequelize-cli db:migrate",
    "test": "NODE_ENV=test nyc mocha --require @babel/register --exit",
    "makemigrations": "babel-node ./node_modules/@crystaldelta/sequelize-auto-migrations/bin/makemigration.js",
    "lint": "eslint \"**/*.js\""
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/*.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "extension": [
      ".js"
    ],
    "cache": true,
    "all": true
  },
  "dependencies": {
    "@crystaldelta/sequelize-auto-migrations": "^1.1.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mocha": "^6.2.2",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.0",
    "nyc": "^14.1.1",
    "sequelize": "^6.6.5",
    "sequelize-auto": "^0.8.2",
    "sequelize-cli": "^6.2.0",
    "winston": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "yarn lint"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.7.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0"
  }
}
